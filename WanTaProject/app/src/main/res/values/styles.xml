<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <declare-styleable name="CustomView">
        <attr name="radidus" format="float"/>
        <attr name="rad_x" format="float"/>
        <attr name="rad_y" format="float"/>
    </declare-styleable>
    <declare-styleable name="CustomBgView">
        <attr name="bg_radidus" format="float"/>
        <attr name="bg_rad_x" format="float"/>
        <attr name="bg_rad_y" format="float"/>
    </declare-styleable>
    <declare-styleable name="MyImageView">
        <attr name="ratio" format="float"/>
        <attr name="imgWidth" format="float"/>
        <attr name="imgHeight" format="float"/>
    </declare-styleable>
    <style name="myschedule_current_month_tv">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">30sp</item>
    </style>
    <style name="AnimationPreview">
        <item name="android:windowEnterAnimation">@animator/fade_in</item>
        <item name="android:windowExitAnimation">@animator/fade_out</item>
    </style>

</resources>
